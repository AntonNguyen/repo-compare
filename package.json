{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "Repo Compare",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecobee/repo-compare.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Cory Taylor",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/github": "1.1.0",
    "@types/js-yaml": "3.12.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "prettier": "2.0.4",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  },
  "bugs": {
    "url": "https://github.com/ecobee/repo-compare/issues"
  },
  "homepage": "https://github.com/ecobee/repo-compare#readme",
  "directories": {
    "lib": "lib"
  }
}
